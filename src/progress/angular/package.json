{
	"name": "@nativescript-community/ui-material-progress-angular",
	"main": "index.js",
	"ngPackage": {
		"dest":"../../tempAngular/progress",
		"lib": {
			"entryFile": "index.ts",
			"umdModuleIds": {
				"@nativescript/core": "ns-core",
				"@nativescript/angular": "ns-angular",
				"@nativescript-community/ui-material-progress": "ns-material-progress"
			}
		},
		"whitelistedNonPeerDependencies": [
			"."
		]
	},
	"scripts": {
		"build": "pnpm i && ../../../node_modules/.bin/rimraf ../../tempAngular/progress && ../../../node_modules/.bin/ng-packagr -p package.json && ../../../node_modules/.bin/cpy './../tempAngular/progress/**/*' '../../../../packages/progress/angular' --parents"
	},
	"devDependencies": {
		"@nativescript-community/ui-material-core": "file:../../../packages/core",
		"@nativescript-community/ui-material-progress": "file:../../../packages/progress"
	}
}
