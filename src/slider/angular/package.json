{
	"name": "@nativescript-community/ui-material-slider-angular",
	"main": "index.js",
	"ngPackage": {
		"lib": {
			"entryFile": "index.ts",
			"umdModuleIds": {
				"@nativescript/core": "ns-core",
				"@nativescript/angular": "ns-angular",
				"nativescript-material-slider": "ns-material-slider"
			}
		},
		"whitelistedNonPeerDependencies": [
			"."
		]
	},
	"scripts": {
		"build": "npm i && ../../../node_modules/.bin/ng-packagr -p package.json && cd dist && ../../../../node_modules/.bin/cpy '**/*' '../../../../packages/nativescript-material-slider/angular' --parents"
	},
	"devDependencies": {
		"nativescript-material-slider": "*"
	}
}
