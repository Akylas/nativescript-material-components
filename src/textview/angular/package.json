{
	"name": "nativescript-material-textview-angular",
	"main": "index.js",
	"ngPackage": {
		"lib": {
			"entryFile": "index.ts",
			"umdModuleIds": {
				"@nativescript/core": "ns-core",
				"@nativescript/angular": "ns-angular",
				"nativescript-material-textview": "ns-material-textview"
			}
		},
		"whitelistedNonPeerDependencies": [
			"."
		]
	},
	"scripts": {
		"build": "npm i && ../../../node_modules/.bin/ng-packagr -p package.json && cd dist && ../../../../node_modules/.bin/cpy '**/*' '../../../../packages/nativescript-material-textview/angular' --parents"
	},
	"devDependencies": {
		"nativescript-material-textview": "^3.2.19"
	}
}
