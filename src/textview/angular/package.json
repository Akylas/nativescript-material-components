{
	"name": "@nativescript-community/ui-material-textview-angular",
	"main": "index.js",
	"ngPackage": {
		"dest":"../../tempAngular/textview",
		"lib": {
			"entryFile": "index.ts",
			"umdModuleIds": {
				"@nativescript/core": "ns-core",
				"@nativescript/angular": "ns-angular",
				"@nativescript-community/ui-material-textview": "ns-material-textview"
			}
		},
		"whitelistedNonPeerDependencies": [
			"."
		]
	},
	"scripts": {
		"build": "pnpm i && ../../../node_modules/.bin/rimraf ../../tempAngular/textview && ../../../node_modules/.bin/ng-packagr -p package.json && ../../../node_modules/.bin/cpy './../tempAngular/textview/**/*' '../../../../packages/textview/angular' --parents"
	},
	"devDependencies": {
		"@nativescript-community/ui-material-core": "file:../../../packages/core",
		"@nativescript-community/ui-material-textview": "file:../../../packages/textview"
	}
}
