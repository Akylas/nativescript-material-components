{
	"name": "nativescript-material-textfield-angular",
	"main": "index.js",
	"ngPackage": {
		"lib": {
			"entryFile": "index.ts",
			"umdModuleIds": {
				"@nativescript/core": "ns-core",
				"@nativescript/angular": "ns-angular",
				"nativescript-material-textfield": "ns-material-textfield"
			}
		},
		"whitelistedNonPeerDependencies": [
			"."
		]
	},
	"scripts": {
		"build": "npm i && ../../../node_modules/.bin/ng-packagr -p package.json && cd dist && ../../../../node_modules/.bin/cpy '**/*' '../../../../packages/nativescript-material-textfield/angular' --parents"
	},
	"devDependencies": {
		"nativescript-material-textfield": "*"
	}
}
