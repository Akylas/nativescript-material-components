{
    "name": "nativescript-material-core",
    "version": "3.3.2",
    "description": "Material Core component",
    "main": "./core",
    "sideEffects": false,
    "typings": "./core.d.ts",
    "scripts": {
        "tsc": "../../node_modules/.bin/cpy ../../src/core/core.d.ts ./ && ../../node_modules/.bin/tsc --skipLibCheck -d",
        "tsc-win": "../../node_modules/.bin/cpy ..\\..\\src\\core\\core.d.ts .\\ && ..\\..\\node_modules\\.bin\\tsc --skipLibCheck -d",
        "build": "npm run tsc && ../../node_modules/.bin/rimraf ./build/esm",
        "clean": "../../node_modules/.bin/rimraf ./*.d.ts ./*.js ./*.js.map android"
    },
    "nativescript": {
        "platforms": {
            "android": "6.2.0",
            "ios": "6.2.0"
        }
    },
    "keywords": [
        "NativeScript",
        "JavaScript",
        "Android",
        "iOS"
    ],
    "author": {
        "name": "Martin Guillon",
        "email": "martin@akylas.fr"
    },
    "bugs": {
        "url": "https://github.com/Akylas/nativescript-material-components/issues"
    },
    "license": "Apache-2.0",
    "homepage": "https://github.com/Akylas/nativescript-material-components",
    "readmeFilename": "README.md",
    "gitHead": "72ff18b67d325539de08d5bcf5370396ea646dfb"
}
