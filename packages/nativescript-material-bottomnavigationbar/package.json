{
    "name": "nativescript-material-bottomnavigationbar",
    "version": "2.3.23",
    "description": "NativeScript plugin to add a bottom navigation bar component for Android & iOS",
    "main": "bottomnavigationbar",
    "typings": "bottomnavigationbar.d.ts",
    "nativescript": {
        "platforms": {
            "android": "6.0.0",
            "ios": "6.0.1"
        }
    },
    "scripts": {
        "tsc-win": "tsc && copy ..\\..\\src\\bottomnavigationbar\\bottomnavigationbar.d.ts .\\",
        "tsc": "tsc && cp ../../src/bottomnavigationbar/bottomnavigationbar.d.ts ./",
        "ngc-win": "ngc",
        "ngc": "node --max-old-space-size=8192 ../../node_modules/.bin/ngc",
        "clean": "../../node_modules/.bin/rimraf ./*.d.ts ./*.js ./*.js.map"
    },
    "keywords": [
        "NativeScript",
        "JavaScript",
        "Typescript",
        "Android",
        "iOS",
        "Henry Chavez",
        "Bytpher",
        "Bottom Navigation",
        "Material Design",
        "Tab Navigation"
    ],
    "author": {
        "name": "Henry Chavez",
        "email": "henry19.chavez@gmail.com"
    },
    "bugs": {
        "url": "https://github.com/Akylas/nativescript-material-components/issues"
    },
    "license": "Apache-2.0",
    "homepage": "https://github.com/Akylas/nativescript-material-components",
    "repository": "https://github.com/Akylas/nativescript-material-components",
    "readmeFilename": "README.md",
    "dependencies": {
        "nativescript-material-core": "^2.3.23"
    },
    "devDependencies": {
        "@angular/compiler": "^8.2.0",
        "@angular/compiler-cli": "^8.2.0",
        "@angular/core": "^8.0.0",
        "nativescript-angular": "^8.0.0",
        "prompt": "^1.0.0",
        "rimraf": "^2.6.3",
        "semver": "^5.6.0",
        "tns-core-modules": "~6.0.0",
        "tns-platform-declarations": "~6.0.0",
        "tslint": "^5.18.0",
        "typescript": "~3.4.5"
    }
}
